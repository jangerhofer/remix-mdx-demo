{
  "dependencies": {
    "@remix-run/express": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-remix": "^0.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "license": "",
  "lint-staged": {
    "*.+(js|ts|tsx|css|md|yml)": [
      "prettier --write"
    ],
    "*.+(js|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "name": "remix-starter",
  "private": true,
  "scripts": {
    "build": "remix build",
    "dev": "remix watch",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "postinstall": "remix setup node",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:dev": "cross-env NODE_ENV=development node server/index.js"
  },
  "sideEffects": false
}
